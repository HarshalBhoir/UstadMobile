<?xml version="1.0" encoding="utf-8"?>


<!--
When we want normal text itself to be clickable (e.g. like a hyperlink) but we want to have a button
in the user interface for accessibility purposes
-->
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"
    >

    <solid android:color="@color/text_only_button_background_color"/>

</shape>