<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'
        xmlns:m="urn:import:gwt.material.design.client.ui">

	<!-- Loading resources if needed. -->
    <ui:with field="res" type="com.ustadmobile.port.gwt.client.resources.AppResources"/>   
    
    <!-- This is the Parent View. Anything in the UI that goes along all the views goes here. -->
    <m:MaterialPanel ui:id="applicaiton_material_panel">
    	
		<!-- The container between the header and footer.-->
        <m:MaterialContainer ui:id="application_container">
        
        	<!-- The content -->
            <m:MaterialPanel ui:id="application_panel_content" ui:field="content">
            	<m:MaterialLabel>Content Material Label</m:MaterialLabel>
            </m:MaterialPanel> 	
            
        </m:MaterialContainer>
    </m:MaterialPanel>
    
</ui:UiBinder>
