<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
        "http://www.gwtproject.org/doctype/2.8.0/gwt-module.dtd">
        
<module rename-to='gwtapp'>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.gwtplatform.mvp.MvpWithEntryPoint"/>

	<!-- GWTP's generated Ginjector -->
    <extend-configuration-property name="gin.ginjector.modules"
       	value="com.ustadmobile.port.gwt.client.gin.ClientModule"/>

	<!-- Module Inheritance -->
	
	<!-- Formatting (NumberFormat or DateTimeFormat -->
	<inherits name="com.google.gwt.i18n.I18N"/>
	
	<!-- Logging -->
	<inherits name="com.google.gwt.logging.Logging"/>
	
	<!-- HTTP -->
	<inherits name="com.google.gwt.http.HTTP"/>
	
	<!-- GWT's XML -->
	<inherits name="com.google.gwt.xml.XML"/>
	
	<!-- Resty GWT -->
	<inherits name="org.fusesource.restygwt.RestyGWT"/>
	
	<!-- JSON GWT-->
	<inherits name="org.JsonGWT"/>
	
	<!-- XML Wrapper -->
	<inherits name="org.XmlPullGWT" />
	
	<!-- Core -->
	<inherits name="com.ustadmobile.Core"/>
	
	<!-- Lib -->
	<inherits name="com.ustadmobile.Lib"/>
	
	<!-- Hashtable for GWT (Hashmap) -->
	<inherits name="java.HashmapHashtable" />
	
	<!-- Local HTML5 Storage -->
	<!-- <inherits name="com.google.gwt.storage.client.Storage" />  -->
    
    <!--GWT Material-->
    <inherits name="gwt.material.design.GwtMaterialWithJQueryDebug"/>
    <inherits name="gwt.material.design.themes.ThemeBlue"/>
    <inherits name="gwt.material.design.addins.GwtMaterialAddinsDebug"/>
    <inherits name="gwt.material.design.GwtMaterialTableDebug"/>
    
    <!-- Servlet -->
    <!-- HelloServlet for rusty-gwt testing -->
    <servlet path='/hello-service/*' class='com.ustadmobile.port.gwt.server.HelloServlet'/>
  	
	<!-- Specify the paths for translatable code on client-->
	<source path='../../core/' />
	<source path='../../lib/' />
    <source path='client'/>
    <source path='shared'/>
    <source path='xmlpull'/>
    <!-- <source path='org.json'/> Commenting because JsonGWT inherits already has it-->
    
    <!-- Replacement : Deferred Bindings -->
  	<!-- Testing Deferred Binding with normal classes. -->
  	<replace-with class="com.ustadmobile.port.gwt.client.test.ReplaceWithThis">
    	<when-type-is class="com.ustadmobile.port.gwt.client.util.ReplaceWithThis"/>
  	</replace-with>
  	
  	<!-- 
  	<replace-with class="com.ustadmobile.core.util.UMCalendarUtil">
    	<when-type-is class="com.ustadmobile.core.util.UMCalendarUtil"/>
  	</replace-with>
    -->
       
    
    <define-configuration-property name="line.separator" is-multi-valued='false'/>
	<set-configuration-property name="line.separator" value='\n'/>
	
	<!-- AppConfig -->
	<define-configuration-property name="content_dir" is-multi-valued='false'/>
	<set-configuration-property name="content_dir" value=''/>
	
	<define-configuration-property name="login_before_open" is-multi-valued='false'/>
	<set-configuration-property name="login_before_open" value="false"/>
	
	<define-configuration-property name="login_before_download" is-multi-valued='false'/>
	<set-configuration-property name="login_before_download" value="false"/>
	
	<define-configuration-property name="first_dest" is-multi-valued='false'/>
	<set-configuration-property name="first_dest" value="BasePoint?catalog-0-url=entries%3A%2F%2F%2FfindEntriesByContainerFileDirectory&amp;catalog-0-t=81&amp;catalog-1-url=entries%3A%2F%2F%2Fmy_library&amp;catalog-1-t=80" />
	
	<define-configuration-property name="first_dest_login_required" is-multi-valued='false'/>
	<set-configuration-property name="first_dest_login_required" value="false"/>
    
    <set-configuration-property name="CssResource.enableGss" value="true"/>
</module>
