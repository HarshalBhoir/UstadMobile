<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ustadmobile.umtestserver" >

    <application android:allowBackup="true" android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name="com.ustadmobile.port.android.App"
        android:theme="@style/Theme.UstadTheme"
        android:supportsRtl="true">

        <!--
         The path to the asset containing the app runtime configuration.
        -->
        <meta-data android:name="com.ustadmobile.core.appconfig"
            android:value="/com/ustadmobile/core/appconfig.properties"/>

        <activity
            android:name="com.ustadmobile.lib.androidtestserver.view.RemoteTestSlaveServerActivity"
            android:configChanges="orientation|keyboardHidden|keyboard|screenSize|locale"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


    </application>

</manifest>

