{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  },
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "module": "es2015",
    "allowSyntheticDefaultImports": true,
    "types": [
       "jquery",
       "materialize-css"
    ]
  }
}
